      "concepts": [
        {
          "id": 8261,
          "uri": "http://www.ddialliance.org/Specification/DDI-CV/DataType#String/en/1.1",
          "notation": "String",
          "title": "String",
          "definition": "Finite sequences of characters. A character is an atomic unit of written communication; it is not further specified except to note that every character has a corresponding Universal Character Set code point (which is an integer).",
          "previousConcept": 3054,
          "position": 0
        },
        {
          "id": 8238,
          "uri": "http://www.ddialliance.org/Specification/DDI-CV/DataType#NormalizedString/en/1.1",
          "notation": "NormalizedString",
          "title": "Normalized string",
          "definition": "Type of string in which any occurrence of whitespace (including tabs, line feeds, and carriage returns) is replaced by a single space.",
          "previousConcept": 3055,
          "position": 1
        },
        {
          "id": 8241,
          "uri": "http://www.ddialliance.org/Specification/DDI-CV/DataType#Boolean/en/1.1",
          "notation": "Boolean",
          "title": "Boolean",
          "definition": "True or false. Can be represented by 1 and 0 correspondingly.",
